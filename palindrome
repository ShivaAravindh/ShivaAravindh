class palindrome{
    public static void main(String[] args) {
         String name = "tenet";
         String s="";
        for (int i=name.length()-1;i>=0;i--){
            s=s+name.charAt(i);
        }
        if (s.equals(name)){
            System.out.println("palindrome");
        }
        else{
            System.out.println("not a palindrome");
        }
    }
}
